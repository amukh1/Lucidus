dec printf(tmp:char*, ...) -> int;
dec getFile(name:char*) -> char*;
dec malloc(size:int) -> int*;
dec atoi(str:char*) -> int;
dec set(x:int*, y:int, i:int) -> int;
dec get(x:int*, i:int) -> int;
dec print(x:int*) -> int;
dec outFile(name:char*, data:char*) -> int;
dec free(pointer:char*) -> void;
dec to_string(x:int) -> char*;
dec concat(x:char*, y:char*) -> char*;

dec main() -> int; 
def main() -> int {
    printf("hello world\n");
    x:int* := malloc(24);
    set(x, 1, 0);
    set(x, 2, 1);
    set(x, 3, 2);



    // output to file
    out:char* := to_string(get(x, 0));
    out = concat(out, "\n");
    out = concat(out, to_string(get(x, 1)));
    out = concat(out, "\n");
    out = concat(out, to_string(get(x, 2)));

    outFile("output.txt", out);

    return 0;
}
