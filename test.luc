dec printf(tmp:char*, ...) -> int;
dec getFile(name:char*) -> char*;
dec extractNum(nums:char*, i:int) -> int;

dec main() -> int; 
def main() -> int {
    nums:char* := getFile("input.txt");
    // extract 3 numbers from char* nums
    printf("%d + %d + %d = %d\n", extractNum(nums, 0), extractNum(nums, 1), extractNum(nums, 2), extractNum(nums, 0) + extractNum(nums, 1) + extractNum(nums, 2));

    return 0;
}
