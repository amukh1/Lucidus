dec printf(tmp:char*, ...) -> int;

struct Car {
    name: char*,
    brand: char*,
    year: int,
    price: int
}

dec car(name:char*, brand:char*, year:int, price: int) -> int;
def car(name:char*, brand:char*, year:int, price: int) -> int {
    Camaro: Car;
    Camaro.name = name;
    Camaro.brand = brand;
    Camaro.year = year;
    Camaro.price = price;

    printf("Car name: %s\n", *Camaro.name);
    printf("Car brand: %s\n", *Camaro.brand);
    printf("Car year: %d\n", *Camaro.year);
    printf("Car price: %d\n", *Camaro.price);

    return 0;
}
