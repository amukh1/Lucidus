dec printf(tmp:char*, ...) -> int;

dec main() -> int;
def main() -> int {

    x:int := 3;

    y:int* := ptr x;

    printf("%d\n", *y);
    printf("%p\n", ptr x);
    printf("%p\n", ptr x);

    x = 4;

    printf("%d\n", *y);
    printf("%d\n", *(ptr x));

    (*(ptr x)) = 5;

    printf("%d\n", *y);

    return 0;
}