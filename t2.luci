dec printf(tmp:char*, ...) -> int;
dec malloc(size:int) -> int*;

dec loop(n:int) -> bool;

dec main() -> int;
def main() -> int {
    loop(16);
    return 0;
}

def loop(n:int) -> bool {
    i:int = 0;
    sum:int = 0;
    goto while;
    label while:
    if (i > n) -> {
        goto end;
    }
    sum = sum + i;
    i = i + 1;
    goto while;
    label end:
    printf("sum = %d\n", sum);
    return true;
}